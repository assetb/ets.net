<div class="row">
    <table style="width: 100%;">
        <tr>
            <th>Номер заявки</th>
            <th>Дата подачи заявки</th>
            <th>Статус</th>
        </tr>
        <tr ng-repeat="order in orders">
            <td>{{order.number}}</td>
            <td>{{order.date | date : 'dd.MM.yyyy'}}</td>
            <td ng-bind="order['<status>k__BackingField']"></td>
        </tr>
    </table>
</div>
